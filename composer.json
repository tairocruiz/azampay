{
    "name": "taitech/azampay",
    "description": "A Laravel package for integrating your application with AzamPay payment services",
    "type": "library",
    "keywords": [
        "laravel",
        "azampay",
        "payment",
        "mobile-money",
        "tanzania",
        "mno",
        "bank",
        "merchant"
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/routing": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "illuminate/config": "^9.0|^10.0|^11.0",
        "spatie/laravel-activitylog": "^4.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Taitech\\Azampay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Taitech\\Azampay\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Aman Simon Tairo",
            "email": "tairocruise@yahoo.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Taitech\\Azampay\\Providers\\AzampayServiceProvider"
            ],
            "aliases": {
                "Azampay": "Taitech\\Azampay\\Facades\\AzampayFacade"
            }
        }
    },
    "conflict": {
        "laravel/framework": "<9.0"
    },
    "suggest": {
        "laravel/framework": "Required for Laravel integration"
    },
    "keywords": [
        "laravel",
        "azampay",
        "payment",
        "mobile-money",
        "tanzania",
        "mno",
        "bank",
        "merchant"
    ]
}
